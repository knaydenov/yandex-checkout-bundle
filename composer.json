{
    "name": "kna/yandex-checkout-bundle",
    "description": "A Symfony Wrapper for the yandex-money/yandex-checkout-sdk-php library",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Klim Naydenov",
            "email": "knaydenov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": ">=4.0  <5.0",
        "yandex-money/yandex-checkout-sdk-php": "^1.4",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": ">=4.0  <5.0",
        "symfony/yaml": ">=4.0  <5.0",
        "symfony/console": ">=4.0  <5.0",
        "symfony/serializer": ">=4.0 <5.0",
        "php-http/guzzle6-adapter": "^2.0",
        "payum/payum-bundle": "^2.3",
        "kna/payum-yandex-checkout": "dev-master",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/orm": "^2.6"
    },
    "autoload": {
      "psr-4": {
        "Kna\\YandexCheckoutBundle\\": ""
      }
    },
    "suggest": {
        "kna/payum-yandex-checkout": "For integration with Payum",
        "doctrine/doctrine-bundle": "For integration with Doctrine",
        "doctrine/orm": "For integration with Doctrine ORM"
    }
}
