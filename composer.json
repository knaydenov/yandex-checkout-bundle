{
    "name": "kna/yandex-checkout-bundle",
    "description": "A Symfony Wrapper for the yandex-money/yandex-checkout-sdk-php library",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Klim Naydenov",
            "email": "knaydenov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^4.1",
        "yandex-money/yandex-checkout-sdk-php": "^1.4",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1",
        "symfony/yaml": "^4.1",
        "symfony/console": "^4.1",
        "jms/payment-core-bundle": "^1.4"
    },
    "suggest": {
        "jms/payment-core-bundle": "For using JMSPaymentCoreBundle's features"
    },
    "autoload": {
      "psr-4": {
        "Kna\\YandexCheckoutBundle\\": ""
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Kna\\YandexCheckoutBundle\\Tests\\App\\": "Tests/App/src/"
      }
    }
}
